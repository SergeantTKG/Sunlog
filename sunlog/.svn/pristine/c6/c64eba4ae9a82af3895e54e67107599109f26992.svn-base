@charset "UTF-8";
/*
* {
	margin:0 auto; padding:0 auto;
}
*/
body {
	background:#fff none repeat scroll 0 0;
	color:rgb(255, 128, 64);
	font-size:12px;
	line-height:1.5;
	margin:10px 20px;
	padding:0;
	font-family:verdana,arial,helvetica,sans-serif;

}

.clearfix:after {
	content:".";
	display:block;
	visibility:hidden;
	height:0.1px;
	font-size:0.1em;
	line-height:0;
	clear:both;
}
.clearfix {
	display:inline-block;
}
.wrapper{
	height: 1500px;
}
.main-content{
	overflow:hidden;
	width:100%;
	height:100%;
	display: flex;
}
.menu{
	width:49px;
	height:100%;
	float:left;
}
.maindiv {
	display: inline-block;
	width:100%;
	height:100%;
	overflow: hidden;
}
.iframe1{
	border: 0;
	overflow:hidden;
	overflow-x: hidden;
	overflow-y: hidden;
	height: 100%;
	width : 100%;
	top: 0px;
	left: 0px;
	right: 0px;
	bottom: 0px;}
a{
	text-decoration:none;
}
a:link,a:hover,a:active{
	color:rgb(255, 128, 64)

}
a:visited{
	color:rgb(200, 128, 64)

}
